    "tailwindcss": "^3.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": "18.x"
  },
  "resolutions": {
    "vitest": "^2.0.5"
  }
