{
  "name": "@blog/frontend",
  "packageManager": "yarn@4.3.1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "mocks": "yarn dlx tsx watch ./src/app/msw/libs/server.ts",
    "lint": "yarn dlx eslint --fix",
    "format": "yarn prettier --write \"./**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "svgr": "yarn dlx @svgr/cli -d src/assets/svg --ignore-existing --typescript --no-dimensions public/svg"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@react-google-maps/api": "^2.19.3",
    "@react-three/drei": "^9.109.5",
    "@react-three/fiber": "^8.16.8",
    "@types/three": "^0.167.1",
    "antd": "^5.20.0",
    "dayjs": "^1.11.12",
    "ky": "^1.5.0",
    "react": "^18.3.1",
    "react-chatbot-kit": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.0",
    "recoil": "^0.7.7",
    "recoil-persist": "^5.1.0",
    "three": "^0.167.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@prerenderer/renderer-puppeteer": "^1.2.4",
    "@prerenderer/rollup-plugin": "^0.3.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.0.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
