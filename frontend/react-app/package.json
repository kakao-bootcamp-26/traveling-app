{
  "name": "@blog/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "mocks": "npx tsx watch ./src/app/msw/libs/server.ts",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
